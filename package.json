  "bugs": {
    "url": "https://github.com/Psychopoulet/split-frames/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "18.11.9",
    "check-version-modules": "1.3.5",
    "coveralls": "3.1.1",
    "eslint": "8.26.0",
    "husky": "8.0.1",
    "mocha": "10.1.0",
    "nyc": "15.1.0"
  },
  "homepage": "https://github.com/Psychopoulet/split-frames#readme",
  "engines": {
    "node": ">=6.0.0"
