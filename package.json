{

  "name": "split-frames",
  "version": "2.4.5",
  "description": "Split Buffer frames from streams",

  "type": "commonjs",
  "main": "lib/main.js",
  "typings": "lib/index.d.ts",

  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/main.js"
      }
    }
  },

  "scripts": {

    "build": "npx tsc --project \"./tsconfig.json\"",

    "lint": "npx eslint ./lib/**/*.js ./test/**/*.js",
    "check-requires": "npx used-deps-analyzer \"./package.json\" \"./lib\" --no-dev --overkill \"fs-extra\" \"node-promfs\"",
    "check-updates": "npx check-version-modules",
    "unit-tests": "npx nyc --reporter=html --reporter=text mocha",

    "tests": "npm run-script lint && npm run check-requires && npm run-script check-updates && npm run-script unit-tests",
    "ci": "npm run-script tests && npx nyc report --reporter=text-lcov | coveralls"

  },

  "files": [
    "/lib"
  ],
  "engines": {
    "node": ">=16.0.0"
  },

  "dependencies": {},
  "devDependencies": {
    "@types/node": "20.5.0",
    "check-version-modules": "1.4.1",
    "coveralls": "3.1.1",
    "eslint": "8.47.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "used-deps-analyzer": "0.1.6"
  },
  "optionalDependencies": {},

  "husky": {
    "hooks": {
      "pre-commit": "npm run-script lint",
      "pre-push": "npm run-script tests"
    }
  },
  "keywords": [
    "frame",
    "Buffer",
    "buffer",
    "split",
    "formate",
    "parse",
    "escape",
    "bit",
    "stream",
    "Transform",
    "transform",
    "STX",
    "ETX",
    "ACK",
    "WAK",
    "NAK",
    "DLE",
    "industrial",
    "protocol",
    "LRC",
    "MSB",
    "LSB"
  ],
  "author": "SÃ©bastien VIDAL",
  "license": "ISC",

  "homepage": "https://github.com/Psychopoulet/split-frames#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/Psychopoulet/split-frames"
  },
  "bugs": {
    "url": "https://github.com/Psychopoulet/split-frames/issues"
  }

}
